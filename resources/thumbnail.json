  },
  ".*(step|stp|iges|igs)":
  {
    "up": "+Z",
    "ssao": true,
    "comp": "-2",
    "cells": true
  },
  ".*(tif|tiff)":
  {
    "up": "-Y",
    "comp": "-2"
  }
}
