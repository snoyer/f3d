{
  "global":
  {
    "scalars": "",
    "fxaa": true,
    "tone-mapping": true,
    "no-background": true,
    "max-size":100
  },
  ".*(dcm|nrrd|mhd|mha|vti)":
  {
    "volume": true
  },
  ".*(3ds|stl|off)":
  {
    "up": "+Z"
  },
  ".*(dxf)":
